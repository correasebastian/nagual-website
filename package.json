{
  "name": "nagual-web",
  "version": "1.0.0",
  "description": "Nagual Website",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run test",
    "cm": "git cz",
    "prepush": "npm run test",
    "prebuild:dev": " rimraf temp",
    "lint": "eslint  src buildScripts",
    "build:dev": "NODE_ENV=development webpack --env.dev -w",
    "prebuild": "rimraf dist",
    "build": "BABEL_ENV=production NODE_ENV=production webpack --env.prod -p",
    "predev:server": "npm run lint",
    "dev:server": "node buildScripts/dev-server.js",
    "security:check": "nsp check",
    "predeploy": "npm run build",
    "deploy": "surge -p dist -d ",
    "deploy:staging": "npm run deploy --   https://ngl-staging.surge.sh",
    "pretest": "npm run lint",
    "test": "BABEL_ENV=testing mocha --require babel-register --reporter progress  \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sawyer-effect/nagual-web.git"
  },
  "keywords": [
    "Nagual",
    "demandware",
    "salesforce",
    "commerce",
    "cloud",
    "sawyer"
  ],
  "author": "sawyer effect",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sawyer-effect/nagual-web/issues"
  },
  "homepage": "https://github.com/sawyer-effect/nagual-web#readme",
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "https://registry.npmjs.org/babel-register/-/babel-register-6.24.1.tgz",
    "chai": "3.5.0",
    "commitizen": "2.9.6",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.0",
    "cz-conventional-changelog": "https://registry.npmjs.org/cz-conventional-changelog/-/cz-conventional-changelog-2.0.0.tgz",
    "eslint": "3.19.0",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.3",
    "inline-manifest-webpack-plugin": "3.0.1",
    "jsdom": "9.12.0",
    "mocha": "3.2.0",
    "node-sass": "4.5.2",
    "nsp": "2.6.3",
    "open": "0.0.5",
    "postcss-loader": "1.3.3",
    "progress-bar-webpack-plugin": "1.9.3",
    "purifycss-webpack": "0.6.0",
    "sass-loader": "6.0.3",
    "sinon": "2.1.0",
    "style-loader": "0.16.1",
    "surge": "0.19.0",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-config-utils": "2.3.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.18.0"
  },
  "engines": {
    "node": "6.10.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
